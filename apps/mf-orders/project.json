{
  "name": "mf-orders",
  "projectType": "application",
  "sourceRoot": "apps/mf-orders/src",
  "targets": {
    "dev": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "mf-orders:build",
        "port": 3001
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/mf-orders"
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "mf-orders:build",
        "port": 4301
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:lob", "lob:retail"]
}
